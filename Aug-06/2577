#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>


int main(void) {
	int A, B, C;
	int mul;
	int tmp;
	int cnt[10];

	for (int i = 0; i < 10; i++) {
		cnt[i] = 0;
	}

	scanf("%d", &A);
	scanf("%d", &B);
	scanf("%d", &C);

	mul = A * B * C;

	while (1) {
		tmp = mul % 10;
		cnt[tmp]++;
		if (mul / 10 == 0) {
			break;
		}
		mul = mul / 10;
	}

	for (int i = 0; i < 10; i++) {
		printf("%d", cnt[i]);

		if (i < 9) {
			printf("\n");
		}
	}


	return 0;
}
