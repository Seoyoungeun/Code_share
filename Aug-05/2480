#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
	int a, b, c;
	int res;

	scanf("%d", &a);
	scanf("%d", &b);
	scanf("%d", &c);

	if (a == b && a == c) {
		res = 10000 + a * 1000;
	}

	else if (a == b || a == c || b == c) {
		if (a == b || a == c) {
			res = 1000 + a * 100;
		}
		else {
			res = 1000 + b * 100;
		}
	}

	else {
		if (a > b && a > c) {
			res = a * 100;
		}
		else if (b > a && b > c) {
			res = b * 100;
		}
		else {
			res = c * 100;
		}
	}

	printf("%d", res);

	return 0;
}
