#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
	int arr[9];
	int max=0;
	int idx;

	for (int i = 0; i < 9; i++) {
		scanf("%d", &arr[i]);
	}
	
	for (int i = 0; i < 9; i++) {
		if (max < arr[i]) {
			max = arr[i];
			idx = i + 1;
		}
	}

	printf("%d\n", max);
	printf("%d\n", idx);
}
